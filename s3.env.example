# AWS credentials for S3 access
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=eu-west-1

# S3 configuration
S3_BUCKET=your-bucket-name
S3_ENDPOINT_URL=https://s3.eu-west-1.amazonaws.com  # Optional: for S3-compatible services

# URL configuration
URL_EXPIRATION=3600  # Pre-signed URL expiration in seconds

# CDN to S3 path mapping (helps translate CDN URLs to S3 keys)
CDN_TO_S3_PREFIX=/  # Optional: if S3 keys have a different prefix than CDN paths
